package com.hommy.service.subscribers;

public class AlarmClock extends DevcieSubscriber {

	public AlarmClock(String deviceName) {
		super(deviceName);
	}

	@Override
	public void update() {
			String message = (String) super.gettopic.getUpdate(this);
			if (message == null) {
				System.out.println(deviceName + ":: No new message");
			} else
				System.out.println(deviceName + ":: Consuming message::" + message);

	}

}
