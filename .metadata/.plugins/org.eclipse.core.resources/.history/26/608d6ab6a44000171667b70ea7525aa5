package com.hommy.administrator.dao.objects;

public class Action {

	private int id;
	private String action;

	public Action(int id, String action) {
		this.id = id;
		this.action = action;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	
	public String getAction() {
		return action;
	}

	public void setAction(String action) {
		this.action = action;
	}

}
