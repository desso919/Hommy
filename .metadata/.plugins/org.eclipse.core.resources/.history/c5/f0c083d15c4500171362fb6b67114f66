package com.hommy.service.subscribers;

public class TV extends DevcieSubscriber {

	public TV(String deviceName) {
		super(deviceName);
	}

	@Override
	public void update() {
		String message = (String) topic.getUpdate(this);
		if (message == null) {
			System.out.println(deviceName + ":: No new message");
		} else
			System.out.println(deviceName + ":: Consuming message::" + message);
	}
}
